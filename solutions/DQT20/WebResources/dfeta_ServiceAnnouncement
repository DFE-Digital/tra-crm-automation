var DFE;
(function (dfe) {
    (function (dqt) {
        (function (webResources) {
            (function (serviceannouncement) {
                serviceannouncement.Form_OnLoad = function (executionContext) {
                    if (executionContext != null & executionContext != undefined) {
                        var formContext = executionContext.getFormContext();
                        //var FORM_TYPE_CREATE = 1;
                        var FORM_TYPE_UPDATE = 2;

                        var formType = formContext.ui.getFormType();

                        if (formType == FORM_TYPE_UPDATE) {
                            formContext.getControl('dfeta_name').setDisabled(true);
                        }
                    }
                }
            })(webResources.ServiceAnnouncement = webResources.ServiceAnnouncement || {});
        })(dqt.WebResources = dqt.WebResources || {});
    })(dfe.Dqt = dfe.Dqt || {});
})(DFE = DFE || {});